{
  "name": "PodMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "npm-run-all format:*",
    "format:js": "prettier --write {.,**}/*.js -c .prettierrc",
    "format:json": "prettier --write {.,**}/*.json -c .prettierrc",
    "format:md": "prettier --write {.,**}/*.md -c .prettierrc",
    "format:ts": "prettier --write {.,**}/*.{ts,tsx} -c .prettierrc",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint index.js app --fix --ext .js,.ts,.tsx",
    "postinstall": "solidarity",
    "hack:types-react-navigation": "rimraf node_modules/@types/react-navigation/node_modules/@types",
    "hack:types-react-native": "rimraf node_modules/@types/react-native/node_modules/@types",
    "hack:types-react-test-renderer": "rimraf node_modules/@types/react-test-renderer/node_modules/@types",
    "patch": "patch-package",
    "prepare": "npm-run-all patch hack:*",
    "storybook": "storybook start -p 9001 --skip-packager",
    "e2e": "source venv/bin/activate && LOCATION='local' py.test && deactivate",
    "test:e2e": "jest test/__e2e__/**/*.test.ts",
    "test:lint": "eslint index.js app --ext .js,.ts,.tsx",
    "appium": "appium",
    "appium:doctor": "appium-doctor"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "aws-amplify": "^1.1.26",
    "aws-amplify-react-native": "^2.1.12",
    "aws-appsync": "^1.7.2",
    "formik": "^1.5.4",
    "global": "^4.3.2",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.11",
    "mobx": "^4.9.4",
    "mobx-react": "^5.4.3",
    "mobx-state-tree": "^3.12.2",
    "react": "16.8.3",
    "react-apollo": "^2.5.5",
    "react-native": "^0.59.8",
    "react-native-config": "^0.11.7",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "1.1.0",
    "react-native-keychain": "3.0.0",
    "react-native-picker": "^4.3.7",
    "react-native-splash-screen": "3.1.1",
    "react-native-vector-icons": "6.1.0",
    "react-navigation": "3.0.0",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-optional-catch-binding": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@storybook/addon-storyshots": "^4.0.6",
    "@storybook/react-native": "3.4.3",
    "@types/jest": "23.3.2",
    "@types/lodash": "^4.14.123",
    "@types/react": "16.7.7",
    "@types/react-native": "^0.57.60",
    "@types/react-native-elements": "^0.18.0",
    "@types/react-navigation": "2.13.5",
    "@types/react-test-renderer": "16.0.3",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "appium": "^1.13.0",
    "appium-doctor": "^1.10.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.1.0",
    "ignite-bowser": "^4.0.0-beta.4",
    "ignite-elements": "^0.3.0",
    "ignite-vector-icons": "^1.1.1",
    "jest": "24.7.1",
    "jest-preset-ignite": "0.6.1",
    "metro-react-native-babel-preset": "0.53.1",
    "npm-run-all": "4.1.5",
    "patch-package": "5.1.1",
    "postinstall-prepare": "1.0.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.10.0",
    "react-devtools-core": "3.4.3",
    "react-dom": "16.5.0",
    "react-powerplug": "0.1.5",
    "react-test-renderer": "16.8.3",
    "reactotron-mst": "^3.1.1",
    "reactotron-react-native": "^3.6.0",
    "rimraf": "2.6.2",
    "solidarity": "2.1.0",
    "typescript": "3.2.4",
    "wd": "^1.11.2"
  },
  "jest": {
    "preset": "jest-preset-ignite",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && git add ."
    }
  },
  "rnpm": {
    "assets": [
      "./app/theme/fonts/"
    ]
  }
}
